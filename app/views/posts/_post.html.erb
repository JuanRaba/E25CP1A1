<tr id="<%= "post-#{post.id}" %>" >
  <td><%= post.titulo %></td>
  <td><%= image_tag post.photo.to_s, height:100 %></td>
  <td><%= link_to 'Show', post %></td>
  <td><%= link_to 'Edit', edit_post_path(post) %></td>
  <td><%= link_to 'Destroy',
                  post,
                  method: :delete,
                  data: { confirm: 'Are you sure?' },
                  remote: true %></td>
</tr>
